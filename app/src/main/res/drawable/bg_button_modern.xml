<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="#30FFFFFF">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white" />
            <corners android:radius="12dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#E8F5E9" android:endColor="#C8E6C9" android:angle="135" android:type="linear" />
            <corners android:radius="12dp" />
            <stroke android:width="1dp" android:color="#A5D6A7" />
        </shape>
    </item>
</ripple>
